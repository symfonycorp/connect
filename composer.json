{
    "name": "symfonycorp/connect",
    "description": "SymfonyConnect SDK",
    "keywords": ["SymfonyConnect", "SDK"],
    "homepage": "https://github.com/symfonycorp/connect",
    "type": "library",
    "license": "MIT",
    "authors": [],
    "require": {
        "php": ">=8.0.2",
        "ext-dom": "*",
        "ext-json": "*",
        "symfony/http-client": "^5.4|^6.0"
    },
    "require-dev": {
        "symfony/form": "^5.4|^6.0",
        "symfony/security-bundle": "^5.4|^6.0",
        "symfony/phpunit-bridge": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "SymfonyCorp\\Connect\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymfonyCorp\\Connect\\Test\\": ["tests/"]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        }
    }
}
