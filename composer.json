{
    "name": "sensiolabs/connect",
    "description": "SensioLabs Connect SDK",
    "keywords": ["SensioLabs Connect", "SDK"],
    "homepage": "https://github.com/sensiolabs/connect",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Weistroff",
            "email": "marc.weistroff@sensiolabs.com",
            "homepage": "http://sensiolabs.com"
        }
    ],
    "require": {
        "guzzle/http": "~3.0",
        "psr/log": "~1.0",
        "guzzle/plugin-cache": "~3.0",
        "guzzle/plugin-backoff": "~3.0"
    },
    "require-dev": {
        "symfony/form": "~2.1",
        "guzzle/plugin-mock": "~3.0",
        "doctrine/cache": "~1.0"
    },
    "suggest": {
        "symfony/security": "To use in a Symfony environment",
        "doctrine/cache": "To get the benefit of HTTP cache"
    },
    "autoload": {
        "psr-0": {
            "SensioLabs": ["src/"]
        },
        "files": ["src/SensioLabs/Connect/functions.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}
